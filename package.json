{
	"name": "congratulation",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "node ./bin/www",
		"dev": "nodemon ./bin/www",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"migrate:run": "sequelize db:migrate",
		"migrate:undo": "sequelize db:migrate:undo",
		"migrate:reset": "sequelize db:migrate:undo:all",
		"migrate": "npm run migrate:reset && npm run migrate:run",
		"seed:run": "sequelize db:seed",
		"seed:all": "sequelize db:seed:all",
		"seed:reset": "sequelize db:seed:undo:all",
		"seed": "npm run seed:reset && npm run seed:all",
		"migrate:seed": "npm run migrate && npm run seed:all",
		"model:generate": "sequelize model:generate"
	},
	"dependencies": {
		"@joi/date": "^2.1.0",
		"axios": "^1.1.3",
		"cookie-parser": "~1.4.4",
		"debug": "~2.6.9",
		"dotenv": "^16.0.3",
		"ejs": "~2.6.1",
		"express": "~4.16.1",
		"http-errors": "~1.6.3",
		"joi": "^17.7.0",
		"moment-timezone": "^0.5.39",
		"morgan": "~1.9.1",
		"node-schedule": "^2.1.0",
		"pg": "^8.8.0",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.25.5"
	},
	"devDependencies": {
		"eslint": "^8.2.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-node": "^4.1.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"nodemon": "^2.0.20",
		"prettier": "^2.7.1"
	}
}
